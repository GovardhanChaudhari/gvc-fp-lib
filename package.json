{
  "name": "gvc-fp-lib",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "main": "dist/index",
  "types": "dist/index",
  "module": "dist",
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc",
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "crt": "ts-node src/RamdaTest.ts",
    "test": "jest",
    "td": "jest --watch",
    "tr": "ts-node-dev --respawn --transpileOnly --no-notify src/RamdaTest.ts",
    "update-log": "auto-changelog -p --output HISTORY.md"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.4.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.2"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "auto-changelog": "^2.2.0",
    "lodash": "^4.17.15",
    "ramda": "^0.27.0"
  },
  "auto-changelog": {
    "output": "HISTORY.md",
    "unreleased": true,
    "commitLimit": false,
    "issuePattern": ".*",
    "template": "keepachangelog"
  }
}
